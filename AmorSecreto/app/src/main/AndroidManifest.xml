<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ltj.joao.amorsecreto">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.Main" />
        <activity
            android:name=".view.Dashboard"
            android:theme="@style/NoAction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.TEste"
            android:theme="@style/NoAction" />
        <activity
            android:name=".view.Agenda"
            android:label="@string/agenda" />
        <activity android:name=".view.InserirImagem" />
        <activity android:name=".view.AgendaInsert" />
        <activity
            android:name=".view.ImagemView"
            android:label="@string/fotos" />
        <activity android:name=".view.InserirMusica"></activity>
    </application>

</manifest>